<?xml version="1.0" encoding="UTF-8"?>
<CompositeObservableProperty xmlns="http://inspire.ec.europa.eu/schemas/omop/2.9"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:gml="http://www.opengis.net/gml/3.2"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  gml:id="prop-1"
  xsi:schemaLocation="http://inspire.ec.europa.eu/schemas/omop/2.9 http://inspire.ec.europa.eu/draft-schemas/omop/2.9/ObservableProperties.xsd">
  <count>4</count>
    <component>
      <ObservableProperty gml:id="rscal">
        <label>Alert of the road condition</label>
      	<basePhenomenon>Road surface condition</basePhenomenon>
      	
      	<statisticalMeasure>     
          <StatisticalMeasure gml:id="stat-instant-PT1M-rscal---">
            <statisticalFunction>instant</statisticalFunction><aggregationTimePeriod>PT1M</aggregationTimePeriod>
          </StatisticalMeasure>
        </statisticalMeasure>
      </ObservableProperty>
    </component>  
    <component>
      <ObservableProperty gml:id="rscif">
        <label>Icefrequency</label>
      	<basePhenomenon>Road surface condition</basePhenomenon>
      	<uom uom="Hz"/>
      	<statisticalMeasure>     
          <StatisticalMeasure gml:id="stat-instant-PT1M-rscif---">
            <statisticalFunction>instant</statisticalFunction><aggregationTimePeriod>PT1M</aggregationTimePeriod>
          </StatisticalMeasure>
        </statisticalMeasure>
      </ObservableProperty>
    </component>  
    <component>
      <ObservableProperty gml:id="rscst">
        <label>State of the road condition</label>
      	<basePhenomenon>Road surface condition</basePhenomenon>
      	
      	<statisticalMeasure>     
          <StatisticalMeasure gml:id="stat-instant-PT1M-rscst---">
            <statisticalFunction>instant</statisticalFunction><aggregationTimePeriod>PT1M</aggregationTimePeriod>
          </StatisticalMeasure>
        </statisticalMeasure>
      </ObservableProperty>
    </component>  
    <component>
      <ObservableProperty gml:id="rsil">
        <label>Ice layer</label>
      	<basePhenomenon>Road surface condition</basePhenomenon>
      	<uom uom="mm"/>
      	<statisticalMeasure>     
          <StatisticalMeasure gml:id="stat-instant-PT1M-rsil---">
            <statisticalFunction>instant</statisticalFunction><aggregationTimePeriod>PT1M</aggregationTimePeriod>
          </StatisticalMeasure>
        </statisticalMeasure>
      </ObservableProperty>
    </component>  </CompositeObservableProperty>
